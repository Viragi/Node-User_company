{
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "handle": {
      "$id": "/properties/handle",
      "type": "string",
      "title": "The Handle Schema ",
      "default": "",
      "examples": ["Michael"],
      "minLength": 1,
      "maxLength": 55
    },
    "password": {
      "$id": "/properties/password",
      "type": "string",
      "title": "The Password Schema ",
      "default": "",
      "examples": ["foo123"],
      "minLength": 1,
      "maxLength": 55
    },
    "name": {
      "$id": "/properties/name",
      "type": "string",
      "title": "The Name Schema ",
      "default": "",
      "examples": ["hueter"],
      "minLength": 1,
      "maxLength": 55
    },
    "logo": {
      "$id": "/properties/logo",
      "type": "string",
      "title": "The Logo Schema ",
      "default": "",
      "examples": [
        "https://avatars0.githubusercontent.com/u/13444851?s=460&v=4"
      ],
      "minLength": 1,
      "maxLength": 55
    }
  },
  "required": ["handle", "password", "name", "logo"]
}
