{
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "first_name": {
      "$id": "/properties/first_name",
      "type": "string",
      "title": "The First_name Schema ",
      "default": "",
      "examples": ["Michael"],
      "minLength": 1,
      "maxLength": 55
    },
    "last_name": {
      "$id": "/properties/last_name",
      "type": "string",
      "title": "The Last_name Schema ",
      "default": "",
      "examples": ["Hueter"],
      "minLength": 1,
      "maxLength": 55
    },
    "username": {
      "$id": "/properties/username",
      "type": ["string", "number"],
      "title": "The Username Schema ",
      "default": "",
      "examples": ["hueter"],
      "minLength": 1,
      "maxLength": 55
    },
    "email": {
      "$id": "/properties/email",
      "type": "string",
      "title": "The Email Schema ",
      "default": "",
      "examples": ["michael@rithmschool.com"],
      "minLength": 1,
      "maxLength": 55
    },
    "password": {
      "$id": "/properties/password",
      "type": "string",
      "title": "The Password Schema ",
      "default": "",
      "examples": ["foo123"],
      "minLength": 1,
      "maxLength": 55
    },
    "current_company": {
      "$id": "/properties/current_company",
      "type": "null",
      "title": "The Current_company Schema ",
      "default": null,
      "examples": [null]
    },
    "photo": {
      "$id": "/properties/photo",
      "type": "string",
      "title": "The Photo Schema ",
      "default": "",
      "examples": [
        "https://avatars0.githubusercontent.com/u/13444851?s=460&v=4"
      ],
      "pattern": ""
    }
  },
  "required": ["username", "email", "password"]
}
